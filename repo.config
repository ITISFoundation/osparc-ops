REPO_USER=foo
REPO_PASSWORD=foo
ENVIRONMENT_MANAGER_NODE=some_host_or_IP

SERVICES_USER=admin
SERVICES_PASSWORD=adminadmin
# the machine fully qualified domain name
MACHINE_FQDN=osparc.local
MONITORING_DOMAIN=monitoring.${MACHINE_FQDN}
PORTAINER_DOMAIN=portainer.${MACHINE_FQDN}
STORAGE_DOMAIN=storage.${MACHINE_FQDN}
REGISTRY_DOMAIN=registry.${MACHINE_FQDN}


# define the stack name
PREFIX_STACK_NAME=
# defines the simcore stack name
SIMCORE_STACK_NAME=simcore
# set the image tag to be used from dockerhub
SIMCORE_IMAGE_TAG=master-latest

# S3
ACCESS_KEY_ID=adminadmin
SECRET_ACCESS_KEY=adminadmin
S3_ENDPOINT=osparc.local:10000
S3_BUCKET=${PREFIX_STACK_NAME}-simcore
S3_SECURE=1

# Graylog
GRAYLOG_LOGIN=admin
GRAYLOG_ROOT_PASSWORD=adminadmin


# Postgres
POSTGRES_DB=simcoredb
POSTGRES_ENDPOINT=${PREFIX_STACK_NAME}_postgres:5432
POSTGRES_HOST=${PREFIX_STACK_NAME}_postgres

POSTGRES_USER=scu
POSTGRES_PASSWORD=adminadmin
POSTGRES_PORT=5432

# registry
REGISTRY_USER=admin
REGISTRY_PW=adminadmin
REGISTRY_AUTH=True
REGISTRY_SSL=True
REGISTRY_S3_ACCESS_KEY_ID=adminadmin
REGISTRY_S3_SECRET_ACCESS_KEY=adminadmin
REGISTRY_S3_BUCKET=registry.osparc.io
REGISTRY_S3_ENDPOINT=osparc.local:10000
REGISTRY_S3_SECURE="true"
REGISTRY_AWS_REGION=us-east-1

# Mail server
SMTP_HOST=smtp.osparc.local
SMTP_PORT=25
SMTP_USERNAME=admin
SMTP_PASSWORD=adminadmin
SMTP_TLS_ENABLED=1